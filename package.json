    "react-redux": "^5.0.6",
    "react-transition-group": "^2.2.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-observable": "^0.19.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.2.0",
    "styled-components": "^3.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
